      "variables": [
        {
          "name": "CMAKE_C_COMPILER",
          "value": "${env.MINGW_BIN_ROOT}/gcc.exe",
          "type": "STRING"
        },
        {
          "name": "CMAKE_CXX_COMPILER",
          "value": "${env.MINGW_BIN_ROOT}/g++.exe",
          "type": "STRING"
        },
        {
          "name": "CMAKE_MAKE_PROGRAM",
